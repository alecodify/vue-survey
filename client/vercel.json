{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://vue-survey-server.vercel.app/api/:path*"
    },
    {
      "source": "/view/survey/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/surveys/result/:id",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
